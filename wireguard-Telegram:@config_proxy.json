    "profile": {
        "store-selected": true,
        "store-fake-ip": true
    },
    "hosts": {},
    "dns": {
        "enable": true,
        "listen": "0.0.0.0:1053",
        "ipv6": true,
        "respect-rules": true,
        "use-hosts": true,
        "use-system-hosts": false,
        "nameserver": [
            "1.1.1.1",
            "1.0.0.1"
        ],
        "proxy-server-nameserver": [
            "8.8.8.8"
        ]
    },
    "tun": {
        "enable": true,
        "stack": "mixed",
        "auto-route": true,
        "strict-route": true,
        "auto-detect-interface": true,
        "dns-hijack": [
            "any:53"
        ],
        "mtu": 9000
    },
    "sniffer": {
        "enable": true,
        "force-dns-mapping": true,
        "parse-pure-ip": true,
        "override-destination": false,
        "sniff": {
            "HTTP": {
                "ports": [
                    80,
                    8080,
                    8880,
                    2052,
                    2082,
                    2086,
                    2095
                ]
            },
            "TLS": {
                "ports": [
                    443,
                    8443,
                    2053,
                    2083,
                    2087,
                    2096
                ]
            }
        }
    },
